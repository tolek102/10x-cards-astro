---
import AuthLayout from "../../layouts/AuthLayout.astro";
import { AuthForms } from "@/components/react/auth/AuthForms";

const error = Astro.url.searchParams.get("error");
const registered = Astro.url.searchParams.get("registered");
---

<AuthLayout title="Logowanie - 10x Cards">
  <AuthForms client:only="react" type="login" error={error} registered={registered} />
</AuthLayout>
